public class If_Else
{
	public static void main(String[] args) 
	{
		int i = 10;
		int j = 20;
		if (i>j)
		{
			System.out.println("J is greater");
		}
		else	
		{
			System.out.println("I is greater");
			System.out.println("Code is executed in else Statement");
		}
		System.out.println("Code is out of if else Statements");
			
	}
}
